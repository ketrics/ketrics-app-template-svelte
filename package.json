  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-gzip": "^2.2.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.9.1"
  },
  "dependencies": {
